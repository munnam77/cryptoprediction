# API Keys
VITE_SENTIMENT_API_KEY=your_sentiment_api_key_here
VITE_BINANCE_API_KEY=your_binance_api_key_here
VITE_BINANCE_API_SECRET=your_binance_api_secret_here

# Binance API Configuration
BINANCE_API_KEY=your_api_key_here
BINANCE_API_SECRET=your_api_secret_here

# Environment
NODE_ENV=development

# API Configuration
API_RATE_LIMIT=1200
API_RATE_LIMIT_MS=60000

# WebSocket Configuration
WS_RECONNECT_DELAY=5000
WS_MAX_RECONNECT_ATTEMPTS=5

# Cache Configuration
CACHE_DURATION_PRICE=5000
CACHE_DURATION_KLINES=60000
CACHE_DURATION_MARKET_DATA=300000

# Trading Configuration
MINIMUM_VOLUME_24H=1000000
MINIMUM_MARKET_CAP=10000000

# Performance Monitoring
ENABLE_PERFORMANCE_MONITORING=true
PERFORMANCE_METRICS_INTERVAL=60000

# Error Tracking
ENABLE_ERROR_TRACKING=true
ERROR_RETRY_ATTEMPTS=3
ERROR_RETRY_DELAY=1000

# Optional Configuration
VITE_API_BASE_URL=https://api.sentimentapi.com/v1
VITE_WEBSOCKET_BASE_URL=wss://stream.binance.com:9443/ws